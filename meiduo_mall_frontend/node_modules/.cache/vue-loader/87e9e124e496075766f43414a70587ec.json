     const data = res.data.data || [];\n    \n        // 判空保护\n        if (!Array.isArray(data) || data.length === 0) {\n          console.warn(\"⚠️ 后端未返回任何内容数据\");\n          return;\n        }\n    \n        this.contents = data;\n    \n        // 使用可选链 ?. 防止 undefined 报错\n        this.index_lbt = data[0] || {};\n        this.index_kx = data[1] || {};\n        this.index_ytgg = data[2]?.contents?.[0] || {};\n        this.index_1f_logo = data[3]?.contents?.[0] || {};\n        this.index_1f_ssxp = data[6]?.contents || [];\n        this.index_1f_cxdj = data[7]?.contents || [];\n        this.index_1f_sjpj = data[8]?.contents || [];\n    \n        this.index_2f_logo = data[9]?.contents?.[0] || {};\n        this.index_2f_jjhg = data[12]?.contents || [];\n        this.index_2f_cxdj = data[13]?.contents || [];\n    \n        this.index_3f_logo = data[14]?.contents?.[0] || {};\n        this.index_3f_shyp = data[17]?.contents || [];\n        this.index_3f_cfyp = data[18]?.contents || [];\n      }).catch(err => {\n        console.error(\"❌ 获取首页内容失败：\", err);\n      });\n    }\n  },\n  components: {\n    Container,\n    Category\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\np {\n  margin: 0 0;\n  height: 1.75em;\n  line-height: 1.75em;\n  text-align: left;\n}\np .el-link {\n  margin: 0 0.5em;\n  font-family: Hiragino Sans GB;\n  font-size: 0.8em;\n  padding-left: 0.5em;\n}\n\n.index_kx {\n  position: absolute;\n  z-index: 10;\n  width: inherit;\n  height: 18.8em;\n  bottom: 0.85em;\n  background-color: rgb(209, 194, 194);\n}\n.detail_box {\n  width: 10em;\n  text-align: center;\n  margin: 0.5em 0.75em;\n  padding: 0.5em 0.5em;\n}\n.detail_box .text {\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  font-size: 0.5em;\n  color: grey;\n}\n\n/* ---- */\n</style>\n"]}]}