'\n\nimport VueWechatTitle from \"vue-wechat-title\";\nimport ElementUI from 'element-ui';\nimport { MessageBox, Message } from 'element-ui'\nimport 'element-ui/lib/theme-chalk/index.css';\n\nimport axios from 'axios'\n\n\n// 环境的切换\nif (process.env.NODE_ENV === 'development') {\n  // 本地开发时可以直连 Django 或改成反代都行\n  axios.defaults.baseURL = 'http://127.0.0.1:8000/';\n} else {\n  // 生产环境下走 nginx 反向代理\n  axios.defaults.baseURL = '/api/';\n}\n\n// 默认携带cookie发起请求和保存cookie\naxios.defaults.withCredentials = true\n\n// http request 拦截器\naxios.interceptors.request.use(\n    config => {\n        if (store.state.token) {  // 判断是否存在token，如果存在的话，则每个http header都加上token。本项目使用JWT\n            config.headers.Authorization = `JWT ${store.state.token}`;\n        }\n        return config;\n    },\n    err => {\n        return Promise.reject(err);\n    });\n\n// http response 拦截器\naxios.interceptors.response.use(\n    response => {\n        return response;\n    },\n    error => {\n        let res = error.response\n        if (res) {\n            switch (res.status) {\n                case 401:\n                    MessageBox.confirm(\n                        '您的登录信息已过期，您可以取消以停留在此页，或重新登录',\n                        '系统提示',\n                        {\n                            confirmButtonText: '重新登录',\n                            cancelButtonText: '取消',\n                            type: 'warning'\n                        }\n                    )\n                        .then(() => {\n                            // 返回 401 清除token信息并跳转到登录页面\n                            store.commit('setStatus');\n                            router.replace({\n                                name: 'login',\n                                query: { redirect: router.currentRoute.fullPath }  // 登陆之后再回到当前页面\n                            });\n                            return;\n                        })\n                        .catch(() => {\n                            store.commit('setStatus');\n                            router.go(0);  // 刷新一下当前页面\n                            return;\n                        })\n                    break;\n                case 400:\n                    Message({\n                        message: res.data.msg || '客户端错误',\n                        type: 'error',\n                    })\n                    break;\n\n                case 403:\n                    Message({\n                        message: res.data.msg || '您的权限不足',\n                        type: 'error',\n                    })\n                    break;\n\n                case 404:\n                    Message({\n                        message: res.data.msg || '网络请求不存在',\n                        type: 'error',\n                    })\n                    break;\n\n                case 500:\n                    Message({\n                        message: res.data.msg || '服务器异常',\n                        type: 'error',\n                    })\n                    break;\n\n                case 501:\n                    Message({\n                        message: res.data.msg || '您的操作被取消或不允许提交',\n                        type: 'warning',\n                    })\n                    break;\n\n                default:\n                    Message({\n                        message: res.data.msg || '服务器正在开小差...',\n                        type: 'error',\n                    })\n            }\n        }\n        // return Promise.reject(error.response.data)   // 返回接口返回的错误信息\n        return Promise.reject(error.response)   // 返回接口返回的错误信息\n\n    });\n\nVue.prototype.$axios = axios;    //全局注册，使用方法为:this.$axios\nVue.prototype.$api = api;    //全局注册，使用方法为:this.$api  api封装统一管理\n\n\n//  路由权限判断\nrouter.beforeEach((to, from, next) => {\n    if (to.meta.requireAuth) {  // 判断该路由是否需要登录权限\n        if (store.state.token) {  // 通过vuex state获取当前的token是否存在\n            next();\n        }\n        else {\n            \n            next({\n                name: 'login',\n                query: { redirect: to.fullPath }  // 将跳转的路由path作为参数，登录成功后跳转到该路由\n            })\n        }\n    }\n    else {\n        next();\n    }\n})\n\nVue.config.productionTip = false\n\nVue.use(ElementUI);\nVue.use(VueWechatTitle);\n\n\nnew Vue({\n    router,\n    store,\n    render: h => h(App)\n}).$mount('#app')\n"],"mappings":"AAAA,OAAOA,GAAG,MAAM,KAAK;AACrB,OAAOC,GAAG,MAAM,WAAW;AAC3B,OAAOC,MAAM,MAAM,UAAU;AAC7B,OAAOC,KAAK,MAAM,SAAS;AAC3B,OAAOC,GAAG,MAAM,OAAO;AAEvB,OAAOC,cAAc,MAAM,kBAAkB;AAC7C,OAAOC,SAAS,MAAM,YAAY;AAClC,SAASC,UAAU,EAAEC,OAAO,QAAQ,YAAY;AAChD,OAAO,sCAAsC;AAE7C,OAAOC,KAAK,MAAM,OAAO;;AAGzB;AACA,IAAIC,OAAO,CAACC,GAAG,CAACC,QAAQ,KAAK,aAAa,EAAE;EAC1C;EACAH,KAAK,CAACI,QAAQ,CAACC,OAAO,GAAG,wBAAwB;AACnD,CAAC,MAAM;EACL;EACAL,KAAK,CAACI,QAAQ,CAACC,OAAO,GAAG,OAAO;AAClC;;AAEA;AACAL,KAAK,CAACI,QAAQ,CAACE,eAAe,GAAG,IAAI;;AAErC;AACAN,KAAK,CAACO,YAAY,CAACC,OAAO,CAACC,GAAG,CAC1BC,MAAM,IAAI;EACN,IAAIhB,KAAK,CAACiB,KAAK,CAACC,KAAK,EAAE;IAAG;IACtBF,MAAM,CAACG,OAAO,CAACC,aAAa,GAAG,OAAOpB,KAAK,CAACiB,KAAK,CAACC,KAAK,EAAE;EAC7D;EACA,OAAOF,MAAM;AACjB,CAAC,EACDK,GAAG,IAAI;EACH,OAAOC,OAAO,CAACC,MAAM,CAACF,GAAG,CAAC;AAC9B,CAAC,CAAC;;AAEN;AACAf,KAAK,CAACO,YAAY,CAACW,QAAQ,CAACT,GAAG,CAC3BS,QAAQ,IAAI;EACR,OAAOA,QAAQ;AACnB,CAAC,EACDC,KAAK,IAAI;EACL,IAAIC,GAAG,GAAGD,KAAK,CAACD,QAAQ;EACxB,IAAIE,GAAG,EAAE;IACL,QAAQA,GAAG,CAACC,MAAM;MACd,KAAK,GAAG;QACJvB,UAAU,CAACwB,OAAO,CACd,6BAA6B,EAC7B,MAAM,EACN;UACIC,iBAAiB,EAAE,MAAM;UACzBC,gBAAgB,EAAE,IAAI;UACtBC,IAAI,EAAE;QACV,CACJ,CAAC,CACIC,IAAI,CAAC,MAAM;UACR;UACAhC,KAAK,CAACiC,MAAM,CAAC,WAAW,CAAC;UACzBlC,MAAM,CAACmC,OAAO,CAAC;YACXC,IAAI,EAAE,OAAO;YACbC,KAAK,EAAE;cAAEC,QAAQ,EAAEtC,MAAM,CAACuC,YAAY,CAACC;YAAS,CAAC,CAAE;UACvD,CAAC,CAAC;UACF;QACJ,CAAC,CAAC,CACDC,KAAK,CAAC,MAAM;UACTxC,KAAK,CAACiC,MAAM,CAAC,WAAW,CAAC;UACzBlC,MAAM,CAAC0C,EAAE,CAAC,CAAC,CAAC,CAAC,CAAE;UACf;QACJ,CAAC,CAAC;QACN;MACJ,KAAK,GAAG;QACJpC,OAAO,CAAC;UACJqC,OAAO,EAAEhB,GAAG,CAACiB,IAAI,CAACC,GAAG,IAAI,OAAO;UAChCb,IAAI,EAAE;QACV,CAAC,CAAC;QACF;MAEJ,KAAK,GAAG;QACJ1B,OAAO,CAAC;UACJqC,OAAO,EAAEhB